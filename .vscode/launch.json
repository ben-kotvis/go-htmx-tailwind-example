    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Package",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}"
        },
        {
            "name": "Launch",
            "type": "go",
            "request": "launch",
            "mode": "exec",
            "program": "${workspaceFolder}/tmp/main"
        },
        {
            "name": "Connect to external session",
            "type": "go",
            "request": "attach",
            "mode": "remote",
            "port": 41581,
            "debugAdapter": "dlv-dap",
            "preLaunchTask": "css-watch"
        }
    ]
}